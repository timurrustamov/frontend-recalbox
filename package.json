{
  "name": "frontend-recalbox",
  "description": "An alternative frontend built with gatsbyjs for [recalbox](https://www.recalbox.com)",
  "version": "1.0.0",
  "private": true,
  "author": "[Timur Rustamov](timur.rustamov@epitech.eu)",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "yarn dev",
    "test": "yarn type-check && yarn lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@material-ui/styles": "next",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.85",
    "gatsby-background-image": "^0.2.72",
    "gatsby-image": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.29",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-json": "^2.1.1",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.17",
    "modern-normalize": "^0.5.0",
    "normalize.css": "^8.0.0",
    "polished": "^2.0.3",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "typeface-montserrat": "^0.0.54",
    "typescript": "^3.3.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.10.1",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-helmet": "^5.0.7",
    "gh-pages": "^2.0.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0"
  }
}
